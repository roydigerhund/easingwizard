{
  "name": "easingwizard",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "turbo run build",
    "build:api": "turbo run build --filter=easingwizard-api",
    "build:frontend": "turbo run build --filter=easingwizard-frontend",
    "build:core": "turbo run build --filter=easingwizard-core",
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=easingwizard-api",
    "dev:frontend": "turbo run dev --filter=easingwizard-frontend",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "generate": "turbo run generate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  }
}
